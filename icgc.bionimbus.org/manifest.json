{
  "notes": [
    "This is the icgc environment manifest",
    "That's all I have to say"
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "arborist": "quay.io/cdis/arborist:2020.03",
    "aws-es-proxy": "abutaha/aws-es-proxy:0.8",
    "dashboard": "quay.io/cdis/gen3-statics:2020.03",
    "fence": "quay.io/cdis/fence:4.17.0",
    "indexd": "quay.io/cdis/indexd:2020.03",
    "peregrine": "quay.io/cdis/peregrine:2020.03",
    "pidgin": "quay.io/cdis/pidgin:2020.03",
    "revproxy": "quay.io/cdis/nginx:1.17.6-ctds-1.0.1",
    "sheepdog": "quay.io/cdis/sheepdog:2020.03",
    "portal": "quay.io/cdis/data-portal:2020.03",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "spark": "quay.io/cdis/gen3-spark:2020.03",
    "tube": "quay.io/cdis/tube:2020.03",
    "guppy": "quay.io/cdis/guppy:2020.03"
  },
  "google": {
    "enabled": "no"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "global": {
    "environment": "icgc",
    "hostname": "icgc.bionimbus.org",
    "revproxy_arn": "arn:aws:acm:us-east-1:446046036926:certificate/cf0052c5-18cc-43f3-9ae7-920568c73247",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/tcga_dictionary/1.0.3/schema.json",
    "portal_app": "gitops",
    "kube_bucket": "kube-icgc-gen3",
    "logs_bucket": "logs-icgc-gen3",
    "useryaml_s3path": "s3://cdis-gen3-users/genomel/user.yaml",
    "public_datasets": true,
    "tier_access_level": "libre",
    "tier_access_limit": 50,
    "sync_from_dbgap": "True",
    "maintenance_mode": "off"
  },
  "canary": {
    "default": 0
  },
  "guppy": {
    "indices": [
      {
        "index": "icgc_etl",
        "type": "subject"
      },
      {
        "index": "icgc_file",
        "type": "file"
      }
    ],
    "config_index": "icgc_array-config",
    "auth_filter_field": "auth_resource_path"
  },
  "scaling": {
    "arborist": {
      "strategy": "auto",
      "min": 1,
      "max": 4,
      "targetCpu": 40
    },
    "portal": {
      "strategy": "auto",
      "min": 1,
      "max": 4,
      "targetCpu": 40
    },
    "fence": {
      "strategy": "auto",
      "min": 1,
      "max": 4,
      "targetCpu": 40
    },
    "indexd": {
      "strategy": "auto",
      "min": 1,
      "max": 4,
      "targetCpu": 40
    },
    "peregrine": {
      "strategy": "auto",
      "min": 1,
      "max": 4,
      "targetCpu": 40
    },
    "presigned-url-fence": {
      "strategy": "auto",
      "min": 1,
      "max": 4,
      "targetCpu": 40
    },
    "revproxy": {
      "strategy": "auto",
      "min": 1,
      "max": 4,
      "targetCpu": 40
    },
    "sheepdog": {
      "strategy": "auto",
      "min": 1,
      "max": 4,
      "targetCpu": 40
    },
    "guppy": {
      "strategy": "auto",
      "min": 1,
      "max": 4,
      "targetCpu": 40
    }
  }
