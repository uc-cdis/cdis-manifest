        "resources": {
          "limits": {
            "memory": "8Gi",
            "cpu": "800m"
          }
        }
      },
      "s3sidecar": {
        "name": "mariner-s3sidecar",
        "image": "quay.io/cdis/mariner-s3sidecar:2021.03",
        "pull_policy": "always",
        "command": [
          "/sidecar"
        ],
        "resources": {
          "limits": {
            "memory": "4Gi",
            "cpu": "500m"
          }
        },
        "securitycontext": {
          "privileged": true
        }
      },
      "gen3fusesidecar": {
        "name": "mariner-gen3fusesidecar",
        "image": "quay.io/cdis/gen3fuse-sidecar:2021.03",
        "pull_policy": "always",
        "command": [
          "/bin/bash",
          "/marinerRun.sh"
        ],
        "lifecycle": {
          "prestop": [
            "/bin/bash",
            "-c",
            "fusermount -u -z /$COMMONS_DATA"
          ]
        },
        "resources": {
          "limits": {
            "memory": "256Mi",
            "cpu": "100m"
          }
        },
        "securitycontext": {
          "privileged": true
        }
      },
      "task": {
        "name": "mariner-task",
        "image": "alpine",
        "pull_policy": "always",
        "command": [
          "/bin/sh"
        ],
        "resources": {
          "limits": {
            "memory": "512Mi",
            "cpu": "900m"
          }
        }
      }
    }
