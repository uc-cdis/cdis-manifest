      "task": {
        "labels": {
          "app": "mariner-task"
        },
        "restart_policy": "on_failure"
      }
    },
    "containers": {
      "engine": {
        "name": "mariner-engine",
        "image": "quay.io/cdis/mariner-engine:2021.03",
        "pull_policy": "always",
        "command": [
          "/mariner",
          "run",
          "$(RUN_ID)"
