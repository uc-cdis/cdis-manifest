{
  "notes": [
    "This is the nightly build environment manifest",
    "The most awesome manifest ever!"
  ],
  "jenkins": {
    "autodeploy": "no"
  },
  "versions": {
    "ambassador": "quay.io/datawire/ambassador:1.4.2",
    "arborist": "quay.io/cdis/arborist:master",
    "audit-service": "quay.io/cdis/audit-service:master",
    "aws-es-proxy": "quay.io/cdis/aws-es-proxy:0.8",
    "awshelper": "quay.io/cdis/awshelper:master",
    "fence": "quay.io/cdis/fence:master",
    "indexd": "quay.io/cdis/indexd:master",
    "mariner": "quay.io/cdis/mariner-server:master",
    "ws-storage": "quay.io/cdis/ws-storage:master",
    "peregrine": "quay.io/cdis/peregrine:master",
    "pidgin": "quay.io/cdis/pidgin:master",
    "revproxy": "quay.io/cdis/nginx:master",
    "sheepdog": "quay.io/cdis/sheepdog:master",
    "portal": "quay.io/cdis/data-portal:master",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "spark": "quay.io/cdis/gen3-spark:master",
    "ssjdispatcher": "quay.io/cdis/ssjdispatcher:master",
    "tube": "quay.io/cdis/tube:master",
    "manifestservice": "quay.io/cdis/manifestservice:master",
    "wts": "quay.io/cdis/workspace-token-service:master",
    "guppy": "quay.io/cdis/guppy:master",
    "sower": "quay.io/cdis/sower:master",
    "hatchery": "quay.io/cdis/hatchery:master",
    "metadata": "quay.io/cdis/metadata-service:master",
    "dashboard": "quay.io/cdis/gen3-statics:master",
    "requestor": "quay.io/cdis/requestor:master"
  },
  "sower": [
    {
      "name": "pelican-export",
      "action": "export",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/pelican-export:master",
        "pull_policy": "Always",
        "env": [
          {
            "name": "DICTIONARY_URL",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "dictionary_url"
              }
            }
          },
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          },
          {
            "name": "ROOT_NODE",
            "value": "subject"
          },
          {
            "name": "EXTRA_NODES",
            "value": "reference_file"
          }
        ],
        "volumeMounts": [
          {
            "name": "pelican-creds-volume",
            "readOnly": true,
            "mountPath": "/pelican-creds.json",
            "subPath": "config.json"
          },
          {
            "name": "peregrine-creds-volume",
            "readOnly": true,
            "mountPath": "/peregrine-creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "12Gi"
      },
      "volumes": [
        {
          "name": "pelican-creds-volume",
          "secret": {
            "secretName": "pelicanservice-g3auto"
          }
        },
        {
          "name": "peregrine-creds-volume",
          "secret": {
            "secretName": "peregrine-creds"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "pelican-export-files",
      "action": "export-files",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/pelican-export:master",
        "pull_policy": "Always",
        "env": [
          {
            "name": "DICTIONARY_URL",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "dictionary_url"
              }
            }
          },
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          },
          {
            "name": "ROOT_NODE",
            "value": "file"
          }
        ],
        "volumeMounts": [
          {
            "name": "pelican-creds-volume",
            "readOnly": true,
            "mountPath": "/pelican-creds.json",
            "subPath": "config.json"
          },
          {
            "name": "peregrine-creds-volume",
            "readOnly": true,
            "mountPath": "/peregrine-creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "12Gi"
      },
      "volumes": [
        {
          "name": "pelican-creds-volume",
          "secret": {
            "secretName": "pelicanservice-g3auto"
          }
        },
        {
          "name": "peregrine-creds-volume",
          "secret": {
            "secretName": "peregrine-creds"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "ingest-metadata-manifest",
      "action": "ingest-metadata-manifest",
      "activeDeadlineSeconds": 86400,
      "serviceAccountName": "jobs-gen3-theanvil-io",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/metadata-manifest-ingestion:master",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "get-dbgap-metadata",
      "action": "get-dbgap-metadata",
      "serviceAccountName": "jobs-gen3-theanvil-io",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/get-dbgap-metadata:master",
        "pull_policy": "Always",
        "env": [],
        "volumeMounts": [
          {
            "name": "creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    }
  ],
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "peregrine": {
    "sidecar": "False"
  },
  "global": {
    "environment": "qaplanetv1",
    "hostname": "nigthly.planx-pla.net",
    "revproxy_arn": "arn:aws:acm:us-east-1:707767160287:certificate/c676c81c-9546-4e9a-9a72-725dd3912bc8",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/anvil/2.3.6/schema.json",
    "portal_app": "gitops",
    "kube_bucket": "kube-qaplanetv1-gen3",
    "logs_bucket": "logs-qaplanetv1-gen3",
    "sync_from_dbgap": "True",
    "useryaml_s3path": "s3://cdis-gen3-users/qa/user.yaml",
    "dispatcher_job_num": "5",
    "netpolicy": "on",
    "mutatedEnvHostname": "gen3.theanvil.io"
  },
  "ssjdispatcher": {
    "job_images": {
      "indexing": "quay.io/cdis/indexs3client:master"
    }
  },
  "guppy": {
    "indices": [
      {
        "index": "gen3.theanvil.io_subject",
        "type": "subject"
      },
      {
        "index": "gen3.theanvil.io_file",
        "type": "file"
      }
    ],
    "auth_filter_field": "auth_resource_path",
    "config_index": "gen3.theanvil.io_array-config"
  },
  "scaling": {
    "arborist": {
      "strategy": "auto"
    },
    "portal": {
      "strategy": "auto",
      "min": 1,
      "max": 1
    },
    "fence": {
      "strategy": "auto"
    },
    "presigned-url-fence": {
      "strategy": "auto",
      "min": 2,
      "max": 3,
      "targetCpu": 40
    },
    "indexd": {
      "strategy": "auto"
    },
    "peregrine": {
      "strategy": "auto"
    },
    "revproxy": {
      "strategy": "auto"
    },
    "sheepdog": {
      "strategy": "auto"
    },
    "guppy": {
      "strategy": "auto"
    }
  },
  "portal": null
}
