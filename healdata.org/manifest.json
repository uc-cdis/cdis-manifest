  "arborist": {
    "deployment_version": "2"
  },
  "metadata": {
    "USE_AGG_MDS": true,
    "AGG_MDS_NAMESPACE": "healprod"
  },
  "portal": {
    "GEN3_BUNDLE": "heal"
  },
  "sower": [
    {
      "name": "batch-export",
      "action": "batch-export",
      "activeDeadlineSeconds": 600,
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/batch-export:4.1.0",
        "pull_policy": "Always",
        "labels": {
          "internet": "yes"
        },
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "batch-export-creds-volume",
            "readOnly": true,
            "mountPath": "/batch-export-creds.json",
            "subPath": "config.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "batch-export-creds-volume",
          "secret": {
            "secretName": "batch-export-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    }
  ],
  "ssjdispatcher": {
    "job_images": {
      "indexing": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/indexs3client:2024.02"
    }
  },
  "indexd": {
    "arborist": "true"
  },
  "global": {
    "environment": "healprod",
    "hostname": "healdata.org",
    "revproxy_arn": "arn:aws:acm:us-east-1:205252583234:certificate/8bb3d424-be34-493c-a43b-e2c686c9dc6c",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/heal_dictionary/1.1.0/schema.json",
    "portal_app": "gitops",
    "sync_from_dbgap": "False",
    "kube_bucket": "kube-healprod-gen3",
    "logs_bucket": "logs-healprod-gen3",
    "dispatcher_job_num": "10",
    "netpolicy": "on",
    "tier_access_level": "libre",
    "useryaml_s3path": "s3://cdis-gen3-users/heal/user.yaml",
    "document_url": "https://heal.github.io/platform-documentation",
    "frontend_root": "gen3ff",
    "argocd": "true",
    "pdb": "on",
    "waf_enabled": "true",
    "es7": true
  },
  "canary": {
    "default": 0
  }
}
