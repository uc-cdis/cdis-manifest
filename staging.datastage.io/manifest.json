{
  "notes": [
    "This is the dev environment manifest",
    "That's all I have to say"
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "arborist": "quay.io/cdis/arborist:1.2.1",
    "aws-es-proxy": "abutaha/aws-es-proxy:0.8",
    "fence": "quay.io/cdis/fence:2.8.2.1",
    "indexd": "quay.io/cdis/indexd:1.0.10",
    "peregrine": "quay.io/cdis/peregrine:feat_bagit2",
    "pidgin": "quay.io/cdis/pidgin:1.0.0",
    "revproxy": "quay.io/cdis/nginx:1.15.5-ctds",
    "sheepdog": "quay.io/cdis/sheepdog:1.1.10",
    "portal": "quay.io/cdis/data-portal:2.13.1",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "jupyterhub": "quay.io/occ_data/jupyterhub:master",
    "spark": "quay.io/cdis/gen3-spark:1.0.0",
    "tube": "quay.io/cdis/tube:0.3.7",
    "guppy": "quay.io/cdis/guppy:0.2.0"
  },
  "jupyterhub": {
    "enabled": "yes",
    "containers": [
                  {
                    "name": "Bioinfo - Python/R",
                    "cpu": 0.5,
                    "memory": "256M",
                    "image": "quay.io/occ_data/jupyternotebook:1.7.2"
                  },
                  {
                    "name": "Bioinfo - Python/R",
                    "cpu": 1.0,
                    "memory": "1024M",
                    "image": "quay.io/occ_data/jupyternotebook:1.7.2"
                  },
                  {
                    "name": "Helium COPD Demo",
                    "cpu": 1.0,
                    "memory": "1024M",
                    "image": "ananyak8srenci/autoencoder-copd-demo:1d33030cef16"
                  }
                ]
  },
  "global": {
    "environment": "stagingdatastage",
    "hostname": "staging.datastage.io",
    "revproxy_arn": "arn:aws:acm:us-east-1:895962626746:certificate/4081369d-6d60-44ff-aa07-11bac3618a9a",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/gtexdictionary/3.1.7/schema.json",
    "portal_app": "gitops",
    "kube_bucket": "kube-stagingdatastage-gen3",
    "logs_bucket": "logs-stagingdatastage-gen3",
    "sync_from_dbgap": "False",
    "useryaml_s3path": "s3://cdis-gen3-users/stagingdatastage/user.yaml",
    "public_datasets": true,
    "netpolicy": "on",
    "tier_access_level": "regular",
    "tier_access_limit": "50"
  },
  "canary": {
    "default": 0
  },
  "guppy": {
    "indices": [
      {
        "index": "dcp_etl",
        "type": "case"
      },
      {
        "index": "dcp_file",
        "type": "file"
      }
    ],
    "config_index": "dcp_array-config",
    "auth_filter_field": "auth_resource_path"
  }
}
