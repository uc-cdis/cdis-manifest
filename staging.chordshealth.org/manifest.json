{
  "notes": [
    "NIEHS CHORDS manifest",
    "see https://github.com/NIEHS/cdis-manifest"
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "arborist": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/arborist:2024.12",
    "aws-es-proxy": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/aws-es-proxy:v1.3.1",
    "fence": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/fence:2024.12",
    "indexd": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/indexd:2024.12",
    "peregrine": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/peregrine:2024.12",
    "revproxy": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/nginx:2024.12",
    "sheepdog": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/sheepdog:2024.12",
    "portal": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/data-portal:2024.12",
    "frontend-framework": "quay.io/cdis/frontend-framework:feat_n3c",
    "metadata": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/metadata-service:2024.12",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.15.3-debian-cloudwatch-1.0",
    "spark": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/gen3-spark:2024.12",
    "tube": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/tube:2024.12",
    "guppy": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/guppy:2024.12",
    "sower": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/sower:2024.12",
    "ambassador": "quay.io/datawire/ambassador:1.4.2",
    "manifestservice": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/manifestservice:2024.12",
    "ssjdispatcher": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/ssjdispatcher:2024.12",
    "dashboard": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/gen3-statics:2024.12"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "jupyterhub": {
    "enabled": "no"
  },
  "peregrine": {
    "sidecar": "True"
  },
  "ssjdispatcher": {
    "job_images": {
      "indexing": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/indexs3client:2024.12"
    }
  },
  "sower": [
    {
      "name": "pelican-export",
      "action": "export",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/pelican-export:2024.12",
        "pull_policy": "Always",
        "env": [
          {
            "name": "DICTIONARY_URL",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "dictionary_url"
              }
            }
          },
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          },
          {
            "name": "ROOT_NODE",
            "value": "dataset"
          }
        ],
        "volumeMounts": [
          {
            "name": "pelican-creds-volume",
            "readOnly": true,
            "mountPath": "/pelican-creds.json",
            "subPath": "config.json"
          },
          {
            "name": "peregrine-creds-volume",
            "readOnly": true,
            "mountPath": "/peregrine-creds.json",
            "subPath": "creds.json"
          },
          {
            "name": "indexd-creds-volume",
            "readOnly": true,
            "mountPath": "/indexd-creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "12Gi"
      },
      "volumes": [
        {
          "name": "pelican-creds-volume",
          "secret": {
            "secretName": "pelicanservice-g3auto"
          }
        },
        {
          "name": "peregrine-creds-volume",
          "secret": {
            "secretName": "peregrine-creds"
          }
        },
        {
          "name": "indexd-creds-volume",
          "secret": {
            "secretName": "indexd-creds"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "pelican-export-files",
      "action": "export-files",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/pelican-export:2024.12",
        "pull_policy": "Always",
        "env": [
          {
            "name": "DICTIONARY_URL",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "dictionary_url"
              }
            }
          },
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          },
          {
            "name": "ROOT_NODE",
            "value": "data_file"
          }
        ],
        "volumeMounts": [
          {
            "name": "pelican-creds-volume",
            "readOnly": true,
            "mountPath": "/pelican-creds.json",
            "subPath": "config.json"
          },
          {
            "name": "peregrine-creds-volume",
            "readOnly": true,
            "mountPath": "/peregrine-creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "12Gi"
      },
      "volumes": [
        {
          "name": "pelican-creds-volume",
          "secret": {
            "secretName": "pelicanservice-g3auto"
          }
        },
        {
          "name": "peregrine-creds-volume",
          "secret": {
            "secretName": "peregrine-creds"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "ingest-metadata-manifest",
      "action": "ingest-metadata-manifest",
      "activeDeadlineSeconds": 86400,
      "serviceAccountName": "FIXME",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/metadata-manifest-ingestion:2024.12",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "get-dbgap-metadata",
      "action": "get-dbgap-metadata",
      "serviceAccountName": "FIXME",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/get-dbgap-metadata:2024.12",
        "pull_policy": "Always",
        "env": [],
        "volumeMounts": [
          {
            "name": "creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    }
  ],
  "global": {
    "environment": "chords",
    "hostname": "staging.chords-health.org",
    "revproxy_arn": "arn:aws:acm:us-east-1:048463324059:certificate/b85556ab-b13b-45bf-afd3-1a70b9234b6e",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/chords-health-dictionary/0.1.0/schema.json",
    "portal_app": "gitops",
    "kube_bucket": "kube-gen3-base-vpc-gen3",
    "logs_bucket": "logs-gen3-base-vpc-gen3",
    "sync_from_dbgap": "False",
    "useryaml_s3path": "s3://cdis-gen3-users/staging-chords-health/user.yaml",
    "GEN3_DEBUG": "False",
    "dispatcher_job_num": "10",
    "netpolicy": "on",
    "public_datasets": true,
    "tier_access_level": "libre",
    "argocd": "true",
    "waf_enabled": "true",
    "karpenter": true,
    "es7": true
  },
  "canary": {
    "default": 0
  },
  "guppy": {
    "indices": [
      {
        "index": "chords_geospatial_data_resource",
        "type": "geospatial_data_resource"
      },
      {
        "index": "chords_population_data_resource",
        "type": "population_data_resource"
      },
      {
        "index": "chords_geospatial_tool_resource",
        "type": "geospatial_tool_resource"
      }
    ],
    "config_index": "chords_array-config",
    "auth_filter_field": "auth_resource_path"
  },
  "scaling": {
    "arborist": {
      "strategy": "auto",
      "min": 1,
      "max": 2,
      "targetCpu": 40
    },
    "portal": {
      "strategy": "auto",
      "min": 1,
      "max": 2,
      "targetCpu": 40
    },
    "fence": {
      "strategy": "auto",
      "min": 1,
      "max": 2,
      "targetCpu": 40
    },
    "presigned-url-fence": {
      "strategy": "auto",
      "min": 1,
      "max": 2,
      "targetCpu": 14
    },
    "indexd": {
      "strategy": "auto",
      "min": 1,
      "max": 2,
      "targetCpu": 40
    },
    "peregrine": {
      "strategy": "auto",
      "min": 1,
      "max": 2,
      "targetCpu": 40
    },
    "revproxy": {
      "strategy": "auto",
      "min": 1,
      "max": 2,
      "targetCpu": 40
    },
    "sheepdog": {
      "strategy": "auto",
      "min": 1,
      "max": 2,
      "targetCpu": 40
    },
    "guppy": {
      "strategy": "auto",
      "min": 1,
      "max": 2,
      "targetCpu": 40
    },
    "metadata": {
      "strategy": "auto",
      "min": 1,
      "max": 2,
      "targetCpu": 40
    }
  }
}