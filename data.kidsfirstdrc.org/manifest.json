{
  "notes": [
    "This is the default manifest"
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "arborist": "quay.io/cdis/arborist:2020.05",
    "fence": "quay.io/cdis/fence:2020.05",
    "indexd": "quay.io/cdis/indexd:2020.05",
    "peregrine": "quay.io/cdis/peregrine:2020.05",
    "pidgin": "quay.io/cdis/pidgin:2020.05",
    "revproxy": "quay.io/cdis/nginx:2020.05",
    "sheepdog": "quay.io/cdis/sheepdog:2020.05",
    "portal": "quay.io/cdis/data-portal:2020.05",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "jupyterhub": {
    "enabled": "no"
  },
  "global": {
    "environment": "prodv1",
    "hostname": "data.kidsfirstdrc.org",
    "revproxy_arn": "arn:aws:acm:us-east-1:053927701465:certificate/1e450360-d333-412e-8bd6-6c9122065e62",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/datadictionary/develop/schema.json",
    "useryaml_s3path": "s3://kf-gen3-user/user.yaml",
    "portal_app": "dev",
    "sync_from_dbgap": "True",
    "slack_send_dbgap": "True"
  },
  "canary": {
    "default": 0
  },
  "scaling": {
    "arborist": {
      "strategy": "auto",
      "min": 4,
      "max": 8,
      "targetCpu": 40
    },
    "portal": {
      "strategy": "auto"
    },
    "fence": {
      "strategy": "auto"
    },
    "indexd": {
      "strategy": "auto",
      "min": 8,
      "max": 12,
      "targetCpu": 40
    },
    "peregrine": {
      "strategy": "auto"
    },
    "revproxy": {
      "strategy": "auto"
    },
    "sheepdog": {
      "strategy": "auto"
    }
  }
}
