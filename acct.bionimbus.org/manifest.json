{
  "notes": [
    "This is the ACCOuNT environment manifest",
    "https://acct.bionimbus.org",
    "Last updated by cgmeyer 04-29-2020 to add Guppy, Tube, and Spark for ETL process."
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "arborist": "quay.io/cdis/arborist:2020.04",
    "aws-es-proxy": "abutaha/aws-es-proxy:0.8",
    "fence": "quay.io/cdis/fence:2020.04",
    "indexd": "quay.io/cdis/indexd:2020.04",
    "peregrine": "quay.io/cdis/peregrine:2020.04",
    "pidgin": "quay.io/cdis/pidgin:2020.04",
    "revproxy": "quay.io/cdis/nginx:2020.04",
    "sheepdog": "quay.io/cdis/sheepdog:2020.04",
    "portal": "quay.io/cdis/data-portal:2020.04",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "spark": "quay.io/cdis/gen3-spark:2020.04",
    "tube": "quay.io/cdis/tube:2020.04",
    "guppy": "quay.io/cdis/guppy:2020.04",
    "jupyterhub": "quay.io/occ_data/jupyterhub:2020.04",
    "ssjdispatcher": "quay.io/cdis/ssjdispatcher:2020.04"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "peregrine": {
    "sidecar": "True"
  },
  "jupyterhub": {
    "enabled": "yes"
  },
  "global": {
    "environment": "accountprod",
    "hostname": "acct.bionimbus.org",
    "revproxy_arn": "arn:aws:acm:us-east-1:830067555646:certificate/ada3ae0c-0e2b-4c97-aeac-7f0923e03312",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/acctdictionary/0.5.0/schema.json",
    "portal_app": "gitops",
    "kube_bucket": "kube-accountprod-gen3",
    "logs_bucket": "logs-accountprod-gen3",
    "sync_from_dbgap": "False",
    "dispatcher_job_num": "10",
    "useryaml_s3path": "s3://cdis-gen3-users/accountprod/user.yaml"
  },
  "ssjdispatcher": {
    "job_images": {
      "indexing": "quay.io/cdis/indexs3client:2020.04"
    }
  },
  "canary": {
    "default": 0
  },
  "guppy": {
    "indices": [
      {
        "index": "acct_etl",
        "type": "subject"
      },
      {
        "index": "acct_file",
        "type": "file"
      }
    ],
    "config_index": "acct_array-config",
    "auth_filter_field": "auth_resource_path"
  },
  "scaling": {
    "arborist": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    },
    "fence": {
      "strategy": "auto",
      "min": 2,
      "max": 10,
      "targetCpu": 40
    },
    "presigned-url-fence": {
      "strategy": "auto",
      "min": 2,
      "max": 10,
      "targetCpu": 40
    },
    "guppy": {
    "strategy": "auto",
    "min": 1,
    "max": 4,
    "targetCpu": 40
    },
    "indexd": {
      "strategy": "auto",
      "min": 4,
      "max": 8,
      "targetCpu": 40
    },
    "peregrine": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    },
    "portal": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    },
    "revproxy": {
      "strategy": "auto",
      "min": 2,
      "max": 6,
      "targetCpu": 40
    },
    "sheepdog": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    }
  }
}
