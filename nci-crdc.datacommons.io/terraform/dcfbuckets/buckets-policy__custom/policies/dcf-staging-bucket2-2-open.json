{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowListLocation",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::584476192960:user/fence-bot-test"
      },
      "Action": [
        "s3:GetBucketLocation",
        "s3:ListBucket"
      ],
      "Resource": "arn:aws:s3:::dcf-staging-bucket2-2-open"
    },
    {
      "Sid": "AllowGetObject",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::584476192960:user/fence-bot-test"
      },
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": "arn:aws:s3:::dcf-staging-bucket2-2-open/*"
    }
  ]
}
