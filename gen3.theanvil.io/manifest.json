{
  "notes": [
    "This is the internalstaging environment manifest",
    "That's all I have to say"
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "arborist": "quay.io/cdis/arborist:2020.06",
    "aws-es-proxy": "abutaha/aws-es-proxy:0.8",
    "fence": "quay.io/cdis/fence:2020.06",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "guppy": "quay.io/cdis/guppy:2020.06",
    "hatchery": "quay.io/cdis/hatchery:2020.06",
    "ambassador": "quay.io/datawire/ambassador:1.4.2",
    "indexd": "quay.io/cdis/indexd:2020.06",
    "manifestservice": "quay.io/cdis/manifestservice:2020.06",
    "peregrine": "quay.io/cdis/peregrine:2020.06",
    "pidgin": "quay.io/cdis/pidgin:2020.06",
    "portal": "quay.io/cdis/data-portal:2020.06",
    "revproxy": "quay.io/cdis/nginx:2020.06",
    "sheepdog": "quay.io/cdis/sheepdog:2020.06",
    "sower": "quay.io/cdis/sower:2020.06",
    "spark": "quay.io/cdis/gen3-spark:2020.06",
    "tube": "quay.io/cdis/tube:2020.06",
    "ssjdispatcher": "quay.io/cdis/ssjdispatcher:2020.06",
    "wts": "quay.io/cdis/workspace-token-service:2020.06"
  },
  "global": {
    "environment": "anvilprod",
    "hostname": "gen3.theanvil.io",
    "revproxy_arn": "arn:aws:acm:us-east-1:474789003679:certificate/9fd731e3-3366-4bd0-a3ef-0453dc07289a",
    "portal_app": "gitops",
    "kube_bucket": "kube-anvilprod-gen3",
    "logs_bucket": "logs-anvilprod-gen3",
    "sync_from_dbgap": "True",
    "useryaml_s3path": "s3://cdis-gen3-users/anvil/user.yaml"
  },
  "sower": [
    {
      "name": "pelican-export",
      "action": "export",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/pelican-export:2020.06",
        "pull_policy": "Always",
        "env": [
          {
            "name": "DICTIONARY_URL",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "dictionary_url"
              }
            }
          },
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          },
          {
            "name": "ROOT_NODE",
            "value": "case"
          }
        ],
        "volumeMounts": [
          {
            "name": "pelican-creds-volume",
            "readOnly": true,
            "mountPath": "/pelican-creds.json",
            "subPath": "config.json"
          },
          {
            "name": "peregrine-creds-volume",
            "readOnly": true,
            "mountPath": "/peregrine-creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "12Gi"
      },
      "volumes": [
        {
          "name": "pelican-creds-volume",
          "secret": {
            "secretName": "pelicanservice-g3auto"
          }
        },
        {
          "name": "peregrine-creds-volume",
          "secret": {
            "secretName": "peregrine-creds"
          }
        }
      ],
      "restart_policy": "Never"
    }
  ],
  "google": {
    "enabled": "yes"
  },
  "ssjdispatcher": {
    "job_images": {
      "indexing": "quay.io/cdis/indexs3client:2020.06"
    }
  },
  "arborist": {
    "deployment_version": "2"
  },
  "guppy": {
    "indices": [
      {
        "index": "etl_anvil",
        "type": "case"
      },
      {
        "index": "file_anvil",
        "type": "file"
      }
    ]
  },
  "canary": {
    "default": 0
  },
  "scaling": {
    "arborist": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    },
    "fence": {
      "strategy": "auto",
      "min": 5,
      "max": 15,
      "targetCpu": 40
    },
    "presigned-url-fence": {
      "strategy": "auto",
      "min": 2,
      "max": 5,
      "targetCpu": 40
    },
    "indexd": {
      "strategy": "auto",
      "min": 4,
      "max": 8,
      "targetCpu": 40
    },
    "revproxy": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    }
  },
  "hatchery": {
    "user-namespace": "jupyter-pods",
    "sidecar": {
      "env": {
        "NAMESPACE": "internalanvil",
        "HOSTNAME": "internalstaging.theanvil.io"
      }
    }
  }
}