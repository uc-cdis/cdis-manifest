[
  {
    "name": "manifest-indexing",
    "action": "index-object-manifest",
    "activeDeadlineSeconds": 86400,
    "serviceAccountName": "jobs-data-midrc-org",
    "container": {
      "name": "manifest-indexing-job",
      "image": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/manifest-indexing:2021.08",
      "pull_policy": "Always",
      "env": [
        {
          "name": "GEN3_HOSTNAME",
          "valueFrom": {
            "configMapKeyRef": {
              "name": "manifest-global",
              "key": "hostname"
            }
          }
        }
      ],
      "volumeMounts": [
        {
          "name": "sower-jobs-creds-volume",
          "readOnly": true,
          "mountPath": "/creds.json",
          "subPath": "creds.json"
        }
      ],
      "cpu-limit": "1",
      "memory-limit": "1Gi"
    },
    "volumes": [
      {
        "name": "sower-jobs-creds-volume",
        "secret": {
          "secretName": "sower-jobs-g3auto"
        }
      }
    ],
    "restart_policy": "Never"
  },
  {
    "name": "indexd-manifest",
    "action": "download-indexd-manifest",
    "serviceAccountName": "jobs-data-midrc-org",
    "container": {
      "name": "indexd-manifest-job",
      "image": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/download-indexd-manifest:2021.08",
      "pull_policy": "Always",
      "env": [
        {
          "name": "GEN3_HOSTNAME",
          "valueFrom": {
            "configMapKeyRef": {
              "name": "manifest-global",
              "key": "hostname"
            }
          }
        }
      ],
      "volumeMounts": [
        {
          "name": "sower-jobs-creds-volume",
          "readOnly": true,
          "mountPath": "/creds.json",
          "subPath": "creds.json"
        }
      ],
      "cpu-limit": "1",
      "memory-limit": "1Gi"
    },
    "volumes": [
      {
        "name": "sower-jobs-creds-volume",
        "secret": {
          "secretName": "sower-jobs-g3auto"
        }
      }
    ],
    "restart_policy": "Never"
  }
]
