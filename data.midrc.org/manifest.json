{
  "notes": [
    "This is the dev environment manifest",
    "That's all I have to say"
  ],
  "versions": {
    "ambassador": "quay.io/datawire/ambassador:1.4.2",
    "arborist": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/arborist:2022.01",
    "aws-es-proxy": "quay.io/cdis/aws-es-proxy:v1.3.1",
    "dashboard": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/gen3-statics:2022.01",
    "fence": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/fence:2022.01",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "guppy": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/guppy:2022.01",
    "hatchery": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/hatchery:2022.01",
    "indexd": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/indexd:2022.01",
    "manifestservice": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/manifestservice:2022.01",
    "metadata": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/metadata-service:2022.01",
    "peregrine": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/peregrine:2022.01",
    "pidgin": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/pidgin:2022.01",
    "portal": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/data-portal:2022.01",
    "revproxy": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/nginx:2022.01",
    "sheepdog": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/sheepdog:2022.01",
    "sower": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/sower:2022.01",
    "spark": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/gen3-spark:2022.01",
    "ssjdispatcher": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/ssjdispatcher:2022.01",
    "tube": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/tube:2022.01",
    "wts": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/workspace-token-service:2022.01"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "jupyterhub": {
    "enabled": "no"
  },
  "peregrine": {
    "sidecar": "True"
  },
  "ssjdispatcher": {
    "job_images": {
      "indexing": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/indexs3client:2022.01"
    }
  },
  "sower": [
    {
      "name": "manifest-indexing",
      "action": "index-object-manifest",
      "activeDeadlineSeconds": 86400,
      "serviceAccountName": "jobs-data-midrc-org",
      "container": {
        "name": "manifest-indexing-job",
        "image": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/manifest-indexing:2022.01",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "sower-jobs-creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "sower-jobs-creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "indexd-manifest",
      "action": "download-indexd-manifest",
      "serviceAccountName": "jobs-data-midrc-org",
      "container": {
        "name": "indexd-manifest-job",
        "image": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/download-indexd-manifest:2022.01",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "sower-jobs-creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "sower-jobs-creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    }
  ],
  "guppy": {
    "indices": [
      {
        "index": "midrc_dataset",
        "type": "dataset"
      },
      {
        "index": "midrc_case",
        "type": "case"
      },
      {
        "index": "midrc_imaging_data_file",
        "type": "imaging_data_file"
      },
      {
        "index": "midrc_imaging_study",
        "type": "imaging_study"
      },
      {
        "index": "midrc_ct_scan",
        "type": "ct_scan"
      },
      {
        "index": "midrc_radiography_exam",
        "type": "radiography_exam"
      },
      {
        "index": "midrc_mr_exam",
        "type": "mr_exam"
      }
    ],
    "config_index": "midrc_array-config",
    "auth_filter_field": "auth_resource_path"
  },
  "global": {
    "environment": "midrcprod",
    "hostname": "data.midrc.org",
    "revproxy_arn": "arn:aws:acm:us-east-1:813684607867:certificate/d4bdf325-afce-4e1d-b431-0dd062ee12c5",
    "useryaml_s3path": "s3://cdis-gen3-users/midrc/user.yaml",
    "kube_bucket": "kube-midrcprod-gen3",
    "dispatcher_job_num": "10",
    "logs_bucket": "logs-midrcprod-gen3",
    "dictionary_url": "http://s3.amazonaws.com/dictionary-artifacts/midrc_dictionary/0.6.2/schema.json",
    "portal_app": "gitops",
    "sync_from_dbgap": "False",
    "netpolicy": "on",
    "public_datasets": true,
    "tier_access_level": "libre",
    "tier_access_limit": "50",
    "maintenance_mode": "off",
    "dd_enabled": true
  },
  "canary": {
    "default": 0
  },
  "scaling": {
    "arborist": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    },
    "aws-es-proxy": {
      "strategy": "auto",
      "min": 1,
      "max": 4,
      "targetCpu": 40
    },
    "fence": {
      "strategy": "auto",
      "min": 5,
      "max": 15,
      "targetCpu": 40
    },
    "guppy": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    },
    "indexd": {
      "strategy": "auto",
      "min": 4,
      "max": 8,
      "targetCpu": 40
    },
    "metadata": {
      "strategy": "auto",
      "min": 2,
      "max": 5,
      "targetCpu": 40
    },
    "peregrine": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    },
    "portal": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    },
    "presigned-url-fence": {
      "strategy": "auto",
      "min": 20,
      "max": 25,
      "targetCpu": 40
    },
    "revproxy": {
      "strategy": "auto",
      "min": 4,
      "max": 10,
      "targetCpu": 40
    },
    "sheepdog": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    }
  }
}
