{
  "notes": [
    "This is the dev environment manifest",
    "That's all I have to say"
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "arborist": "quay.io/cdis/arborist:1.2.1",
    "fence": "quay.io/cdis/fence:2.8.2.1",
    "indexd": "quay.io/cdis/indexd:1.1.4",
    "peregrine": "quay.io/cdis/peregrine:1.3.0",
    "pidgin": "quay.io/cdis/pidgin:1.0.0",
    "revproxy": "quay.io/cdis/nginx:1.15.5-ctds",
    "sheepdog": "quay.io/cdis/sheepdog:1.1.13",
    "portal": "quay.io/cdis/data-portal:2.17.0",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "jupyterhub": "quay.io/occ_data/jupyterhub:master",
    "spark": "quay.io/cdis/gen3-spark:1.0.0",
    "tube": "quay.io/cdis/tube:0.3.7",
    "nb2": "quay.io/cdis/shiny-nb2:feat_dcp-shiny-app",
    "hatchery": "quay.io/cdis/hatchery:0.1.0",
    "ambassador": "quay.io/datawire/ambassador:0.60.3",
    "wts": "quay.io/cdis/workspace-token-service:0.1.0",
    "manifestservice": "quay.io/cdis/manifestservice:0.1.1"
  },
  "hatchery": {
    "user-namespace": "jupyter-pods",
    "sub-dir": "/lw-workspace",
    "user-volume-size": "500Gi",
    "sidecar": {},
    "containers": [
      {
        "target-port": 8888,
        "cpu-limit": "1.0",
        "memory-limit": "256Mi",
        "name": "Earth - Python | Tiny",
        "image": "quay.io/occ_data/jupyter-geo:1.8.1",
        "env": {},
        "args": [
          "--NotebookApp.base_url=/lw-workspace/proxy/",
          "--NotebookApp.password=''",
          "--NotebookApp.token=''"
        ],
        "command": [
          "start-notebook.sh"
        ],
        "path-rewrite": "/lw-workspace/proxy/",
        "use-tls": "false",
        "ready-probe": "/lw-workspace/proxy/",
        "lifecycle-post-start": [
          "/bin/sh",
          "-c",
          "export IAM=`whoami`; rm -rf /home/$IAM/pd/dockerHome; ln -s $(pwd) /home/$IAM/pd/dockerHome; mkdir -p /home/$IAM/.jupyter/custom; echo \"define(['base/js/namespace'], function(Jupyter){Jupyter._target = '_self';})\" >/home/$IAM/.jupyter/custom/custom.js; ln -s /data /home/$IAM/pd/; true"
        ],
        "user-uid": 1000,
        "fs-gid": 100,
        "user-volume-location": "/home/jovyan/pd"
      },
      {
        "target-port": 8888,
        "cpu-limit": "4.0",
        "memory-limit": "15512Mi",
        "name": "Earth - Python | Large",
        "image": "quay.io/occ_data/jupyter-geo:1.8.1",
        "env": {},
        "args": [
          "--NotebookApp.base_url=/lw-workspace/proxy/",
          "--NotebookApp.password=''",
          "--NotebookApp.token=''"
        ],
        "command": [
          "start-notebook.sh"
        ],
        "path-rewrite": "/lw-workspace/proxy/",
        "use-tls": "false",
        "ready-probe": "/lw-workspace/proxy/",
        "lifecycle-post-start": [
          "/bin/sh",
          "-c",
          "export IAM=`whoami`; rm -rf /home/$IAM/pd/dockerHome; ln -s $(pwd) /home/$IAM/pd/dockerHome; mkdir -p /home/$IAM/.jupyter/custom; echo \"define(['base/js/namespace'], function(Jupyter){Jupyter._target = '_self';})\" >/home/$IAM/.jupyter/custom/custom.js; ln -s /data /home/$IAM/pd/; true"
        ],
        "user-uid": 1000,
        "fs-gid": 100,
        "user-volume-location": "/home/jovyan/pd"
      },
      {
        "target-port": 8888,
        "cpu-limit": "4.0",
        "memory-limit": "15512Mi",
        "name": "Swirlnet - Python | Medium",
        "image": "djgagne/swirlnet:latest",
        "env": {},
        "args": [
          "--NotebookApp.base_url=/lw-workspace/proxy/",
          "--NotebookApp.password=''",
          "--NotebookApp.token=''"
        ],
        "command": [
          "start-notebook.sh"
        ],
        "path-rewrite": "/lw-workspace/proxy/",
        "use-tls": "false",
        "ready-probe": "/lw-workspace/proxy/",
        "lifecycle-post-start": [
          "/bin/sh",
          "-c",
          "export IAM=`whoami`; rm -rf /home/$IAM/pd/dockerHome; ln -s $(pwd) /home/$IAM/pd/dockerHome; mkdir -p /home/$IAM/.jupyter/custom; echo \"define(['base/js/namespace'], function(Jupyter){Jupyter._target = '_self';})\" >/home/$IAM/.jupyter/custom/custom.js; ln -s /data /home/$IAM/pd/; true"
        ],
        "user-uid": 1000,
        "fs-gid": 100,
        "user-volume-location": "/home/jovyan/pd"
      }
    ]
  },
  "jupyterhub": {
    "enabled": "yes",
    "containers": [
      {
        "name": "Earth - Python",
        "cpu": 0.5,
        "memory": "256M",
        "image": "quay.io/occ_data/jupyter-geo:1.8.1"
      },
      {
        "name": "Earth - Python",
        "cpu": 1.0,
        "memory": "10024M",
        "image": "quay.io/occ_data/jupyter-geo:1.8.1"
      },
      {
        "name": "Earth - Python",
        "cpu": 1.0,
        "memory": "15512M",
        "image": "quay.io/occ_data/jupyter-geo:1.8.1"
      },
      {
        "name": "Swirlnet - Python",
        "cpu": 1.0,
        "memory": "10024M",
        "image": "djgagne/swirlnet:latest"
      }
    ]
  },
  "global": {
    "environment": "edcprodv2",
    "hostname": "portal.occ-data.org",
    "revproxy_arn": "arn:aws:acm:us-east-1:302170346065:certificate/f1260f2b-0893-4eb4-807c-fcd62941b8cf",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/envdictionary/1.0.5/schema.json",
    "portal_app": "gitops",
    "sync_from_dbgap": "False",
    "useryaml_s3path": "s3://cdis-gen3-users/edc/user.yaml",
    "netpolicy": "on"
  },
  "canary": {
    "default": 0
  }
}
