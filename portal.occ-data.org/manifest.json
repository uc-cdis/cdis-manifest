{
  "notes": [
    "This is the occ environment manifest",
    "That's all I have to say"
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "arborist": "quay.io/cdis/arborist:2021.06",
    "fence": "quay.io/cdis/fence:2021.06",
    "indexd": "quay.io/cdis/indexd:2021.06",
    "peregrine": "quay.io/cdis/peregrine:2021.06",
    "revproxy": "quay.io/cdis/nginx:2021.06",
    "sheepdog": "quay.io/cdis/sheepdog:2021.06",
    "portal": "quay.io/cdis/data-portal:2021.06",
    "pidgin": "quay.io/cdis/pidgin:2021.06",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "hatchery": {
    "user-namespace": "jupyter-pods",
    "sub-dir": "/lw-workspace",
    "user-volume-size": "500Gi",
    "sidecar": {
      "cpu-limit": "1.0",
      "memory-limit": "256Mi",
      "image": "quay.io/cdis/gen3fuse-sidecar:2021.06",
      "env": {
        "NAMESPACE": "default",
        "HOSTNAME": "portal.occ-data.org"
      },
      "args": [],
      "command": [
        "/bin/bash",
        "/sidecarDockerrun.sh"
      ],
      "lifecycle-pre-stop": [
        "su",
        "-c",
        "echo test",
        "-s",
        "/bin/sh",
        "root"
      ]
    },
    "containers": [
      {
        "target-port": 8888,
        "cpu-limit": "1.0",
        "memory-limit": "256Mi",
        "name": "Earth - Python | Tiny",
        "image": "quay.io/occ_data/jupyter-geo:1.9.1",
        "env": {},
        "args": [
          "--NotebookApp.base_url=/lw-workspace/proxy/",
          "--NotebookApp.password=''",
          "--NotebookApp.token=''"
        ],
        "command": [
          "start-notebook.sh"
        ],
        "path-rewrite": "/lw-workspace/proxy/",
        "use-tls": "false",
        "ready-probe": "/lw-workspace/proxy/",
        "lifecycle-post-start": [
          "/bin/sh",
          "-c",
          "export IAM=`whoami`; rm -rf /home/$IAM/pd/dockerHome; ln -s $(pwd) /home/$IAM/pd/dockerHome; mkdir -p /home/$IAM/.jupyter/custom; echo \"define(['base/js/namespace'], function(Jupyter){Jupyter._target = '_self';})\" >/home/$IAM/.jupyter/custom/custom.js; ln -s /data /home/$IAM/pd/; true"
        ],
        "user-uid": 1000,
        "fs-gid": 100,
        "user-volume-location": "/home/jovyan/pd"
      },
      {
        "target-port": 8888,
        "cpu-limit": "2.0",
        "memory-limit": "12000Mi",
        "name": "Earth - Python | Large",
        "image": "quay.io/occ_data/jupyter-geo:1.9.1",
        "env": {},
        "args": [
          "--NotebookApp.base_url=/lw-workspace/proxy/",
          "--NotebookApp.password=''",
          "--NotebookApp.token=''"
        ],
        "command": [
          "start-notebook.sh"
        ],
        "path-rewrite": "/lw-workspace/proxy/",
        "use-tls": "false",
        "ready-probe": "/lw-workspace/proxy/",
        "lifecycle-post-start": [
          "/bin/sh",
          "-c",
          "export IAM=`whoami`; rm -rf /home/$IAM/pd/dockerHome; ln -s $(pwd) /home/$IAM/pd/dockerHome; mkdir -p /home/$IAM/.jupyter/custom; echo \"define(['base/js/namespace'], function(Jupyter){Jupyter._target = '_self';})\" >/home/$IAM/.jupyter/custom/custom.js; ln -s /data /home/$IAM/pd/; true"
        ],
        "user-uid": 1000,
        "fs-gid": 100,
        "user-volume-location": "/home/jovyan/pd"
      },
      {
        "target-port": 8888,
        "cpu-limit": "2.0",
        "memory-limit": "12000Mi",
        "name": "Swirlnet - Python | Medium",
        "image": "djgagne/swirlnet:latest",
        "env": {},
        "args": [
          "--NotebookApp.base_url=/lw-workspace/proxy/",
          "--NotebookApp.password=''",
          "--NotebookApp.token=''"
        ],
        "command": [
          "start-notebook.sh"
        ],
        "path-rewrite": "/lw-workspace/proxy/",
        "use-tls": "false",
        "ready-probe": "/lw-workspace/proxy/",
        "lifecycle-post-start": [
          "/bin/sh",
          "-c",
          "export IAM=`whoami`; rm -rf /home/$IAM/pd/dockerHome; ln -s $(pwd) /home/$IAM/pd/dockerHome; mkdir -p /home/$IAM/.jupyter/custom; echo \"define(['base/js/namespace'], function(Jupyter){Jupyter._target = '_self';})\" >/home/$IAM/.jupyter/custom/custom.js; ln -s /data /home/$IAM/pd/; true"
        ],
        "user-uid": 1000,
        "fs-gid": 100,
        "user-volume-location": "/home/jovyan/pd"
      }
    ]
  },
  "global": {
    "environment": "edcprodv2",
    "hostname": "portal.occ-data.org",
    "revproxy_arn": "arn:aws:acm:us-east-1:302170346065:certificate/80374dc8-e2e9-4698-8458-82cafd6f6bc8",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/envdictionary/1.0.5/schema.json",
    "portal_app": "gitops",
    "sync_from_dbgap": "False",
    "useryaml_s3path": "s3://cdis-gen3-users/edc/user.yaml",
    "netpolicy": "on"
  },
  "scaling": {
    "arborist": {
      "strategy": "auto",
      "min": 1,
      "max": 1
    },
    "fence": {
      "strategy": "auto",
      "min": 1,
      "max": 1
    },
    "indexd": {
      "strategy": "auto",
      "min": 1,
      "max": 1
    },
    "revproxy": {
      "strategy": "auto",
      "min": 1,
      "max": 1
    },
    "sheepdog": {
      "strategy": "pin",
      "num": 1
    }
  },
  "canary": {
    "default": 0
  }
}
