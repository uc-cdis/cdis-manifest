              "contentType": "string",
              "includeName": true,
              "includeIfNotAvailable": false
            },
            {
              "name": "Project ID",
              "field": "project_id",
              "contentType": "string",
              "includeIfNotAvailable": false
            },
            {
              "name": "Data Commons",
              "field": "commons",
              "contentType": "string",
              "includeIfNotAvailable": false
            }
          ]
