      "name": "ingest-metadata-manifest",
      "action": "ingest-metadata-manifest",
      "activeDeadlineSeconds": 86400,
      "serviceAccountName": "jobs-preprod-healdata-org",
      "container": {
        "name": "job-task",
        "image": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/metadata-manifest-ingestion:2021.03",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
