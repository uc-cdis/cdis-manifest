{
  "notes": [
    "This is the dev environment manifest",
    "That's all I have to say"
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "arborist": "quay.io/cdis/arborist:2020.09",
    "dashboard": "quay.io/cdis/gen3-statics:2020.09",
    "fence": "quay.io/cdis/fence:4.23.6",
    "indexd": "quay.io/cdis/indexd:2020.09",
    "peregrine": "quay.io/cdis/peregrine:2020.09",
    "pidgin": "quay.io/cdis/pidgin:2020.09",
    "revproxy": "quay.io/cdis/nginx:2020.09",
    "sheepdog": "quay.io/cdis/sheepdog:2020.09",
    "portal": "quay.io/cdis/data-portal:2020.09",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "jupyterhub": {
    "enabled": "no"
  },
  "global": {
    "environment": "skfqa",
    "hostname": "gen3staging.kidsfirstdrc.org",
    "revproxy_arn": "arn:aws:acm:us-east-1:222487244010:certificate/d91de077-c452-4172-9f1e-c88c99feb9f0",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/datadictionary/develop/schema.json",
    "useryaml_s3path": "s3://kf-gen3-user/nonprd/user-nonprd.yaml",
    "portal_app": "dev",
    "sync_from_dbgap": "False",
    "netpolicy": "on",
    "kube_bucket": "kube-skfqa-gen3",
    "logs_bucket": "logs-skfqa-gen3"
  },
  "canary": {
    "default": 0
  },
  "scaling": {
    "arborist": {
      "strategy": "auto"
    },
    "portal": {
      "strategy": "auto"
    },
    "fence": {
      "strategy": "auto"
    },
    "indexd": {
      "strategy": "auto",
      "min": 8,
      "max": 12,
      "targetCpu": 40
    },
    "peregrine": {
      "strategy": "auto"
    },
    "revproxy": {
      "strategy": "auto"
    },
    "sheepdog": {
      "strategy": "auto"
    },
    "presigned-url-fence": {
      "strategy": "auto"
    }
  }
}
